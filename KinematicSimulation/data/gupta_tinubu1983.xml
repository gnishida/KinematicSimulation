<!--
This fourbar linkage is used in the paper
Gupta and Tinubu. 1983. Synthesis of Bimodal Function Generating Mechanisms Without Branch Defect.
-->
<design author="Gen Nishida" version="1.0" date="7/12/2017">
	<joints>
		<joint id="0" type="pin" ground="true" x="-1" y="0"/>
		<joint id="1" type="pin" ground="true" x="0" y="0"/>
		<joint id="2" type="pin" x="-1.48" y="0.9"/>
		<joint id="3" type="pin" x="-0.96" y="1.05"/>
	</joints>
	<links>
		<link joints="0,2" driver="true"/>
		<link joints="1,3"/>
		<link joints="2,3"/>
	</links>
</design>